{
    "manifest_version": 3,
    "name": "Select Intelisens",
    "version": "1.0",
    "description": "Wy≈õwietla okienko po zaznaczeniu tekstu.",
    "permissions": ["scripting"],
    "host_permissions": [
      "https://ipapi.co/",
      "https://latest.currency-api.pages.dev/v1/currencies/"
    ],
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": ["<all_urls>"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["currencyData.json"],
        "matches": ["<all_urls>"]
      }
    ]
  }